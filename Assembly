---
github:
  active: true

gem:
  active : true

dnote:
  labels: ~
  output: log/NOTES.rdoc

yard:
  yardopts: true

qed:
  active: true

qedoc:
  files: spec/
  output: QED.rdoc

readme:
  service: custom
  document: |
    cmd = 'malt README.rdoc > site/readme.html'
    puts cmd
    sh cmd

vclog:
  output:
    - log/ChangeLog.rdoc

email:
  mailto: 
    - ruby-talk@ruby-lang.org
    - rubyworks-mailinglist@googlegroups.com

